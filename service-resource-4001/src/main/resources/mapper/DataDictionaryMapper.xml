<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imooc.mapper.DataDictionaryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.imooc.pojo.DataDictionary">
        <id column="id" property="id" />
        <result column="type_code" property="typeCode" />
        <result column="type_name" property="typeName" />
        <result column="item_key" property="itemKey" />
        <result column="item_value" property="itemValue" />
        <result column="sort" property="sort" />
        <result column="icon" property="icon" />
        <result column="enable" property="enable" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, type_code, type_name, item_key, item_value, sort, icon, enable
    </sql>

</mapper>
